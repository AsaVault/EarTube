@model IEnumerable<SongModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <div class="notify-upload">
        
    </div>

    <div class="">
        <h3 class="h5 text-uppercase">All Songs</h3>
        <br />
        <div>
            @if (ViewBag.IsSuccess == true)
            {
                <div class="alert alert-success alert-dismissible fade show" role="alert">
                    You have added a new <strong>song</strong> successfully. <br />
                    Email successfully sent to your subscribers.
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
            }
        </div>

        @if (Model.Count() > 0)
        {

            <div class="row col-md-12" id="view-all">
                @* Inserted Partial view all *@
                @await Html.PartialAsync("_ViewAll", Model)
                @*<partial name="_ViewAll" model="Model" />*@
            </div>
        }
        else
        {
            <p>No songs</p>
        }
    </div>



@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

}

