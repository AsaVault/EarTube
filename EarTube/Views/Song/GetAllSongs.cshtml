@model IEnumerable<SongModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h3 class="h5 text-uppercase">All Songs</h3>
    <br />
    @if (ViewBag.IsSuccess == true)
    {
        <div class="alert alert-success alert-dismissible fade show" role="alert">
            <strong>Congratulations</strong> You have added new song successfully. <br />
            Click <a asp-controller="Song" asp-action="GetSong" asp-route-id="@ViewBag.SongId">here</a> to view the details
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    }

    <div class="row">
        @*<p>@Model.User.FirstName</p>*@

        @*@foreach (var song in Model)
            {
                <div class="col-md-4">
                    <partial name="_ViewAll" model="@song" />

                </div>
            }*@

        @* Inserted Partial view all *@
        @*@await Html.PartialAsync("_ViewAll", Model);*@
        @*<partial name="_ViewAll" model="Model" />*@

    </div>
</div>

<div class="row" id="view-all">
    @*<p>@Model.User.FirstName</p>*@

    @*@foreach (var song in Model)
        {
            <div class="col-md-4">
                <partial name="_ViewAll" model="@song" />

            </div>
        }*@

    @* Inserted Partial view all *@
    @await Html.PartialAsync("_ViewAll", Model);
    @*<partial name="_ViewAll" model="Model" />*@

</div>


@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

}

